{
  "name": "web-pop-miner",
  "version": "0.2.5",
  "description": "Web PoP miner",
  "main": "index.js",
  "scripts": {
    "build": "npm run copy-popminer-wasm && tsc && vite build",
    "copy-popminer-wasm": "mkdir -p ./src/assets && cp ./node_modules/@hemilabs/pop-miner/dist/popminer.wasm ./src/assets/popminer.wasm",
    "dev": "npm run copy-popminer-wasm && vite",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint --cache .",
    "test": "vitest",
    "tsc": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@hemilabs/pop-miner": "0.4.2",
    "@tanstack/react-query": "5.45.1",
    "camelcase-keys": "7.0.2",
    "esplora-client": "1.1.0",
    "fetch-plus-plus": "1.0.0",
    "rc-tooltip": "6.2.0",
    "react": "18.2.0",
    "react-confetti-explosion": "2.1.2",
    "react-dom": "18.2.0",
    "react-loading-skeleton": "3.4.0",
    "react-router-dom": "6.23.1",
    "react-toastify": "10.0.5",
    "use-debounce": "10.0.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/node": "20.14.9",
    "@types/rc-tooltip": "3.7.14",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "@vitejs/plugin-react": "4.3.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.57.1",
    "eslint-config-bloq": "4.4.1",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-markdownlint": "0.5.0",
    "husky": "9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "15.3.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
    "vite": "5.2.13",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "^2.1.1"
  },
  "type": "module"
}
